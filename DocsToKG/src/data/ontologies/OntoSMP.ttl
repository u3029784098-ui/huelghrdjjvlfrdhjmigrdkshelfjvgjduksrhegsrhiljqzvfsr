###  urn:absolute:OntoSMP#antiColorCharge
:antiColorCharge rdf:type owl:DatatypeProperty ;
                 rdfs:domain :AntiQuark ;
                 rdfs:range [ rdf:type rdfs:Datatype ;
                              owl:oneOf [ rdf:type rdf:List ;
                                          rdf:first "anti-blue" ;
                                          rdf:rest [ rdf:type rdf:List ;
                                                     rdf:first "anti-green" ;
                                                     rdf:rest [ rdf:type rdf:List ;
                                                                rdf:first "anti-red" ;
                                                                rdf:rest [ rdf:type rdf:List ;
                                                                           rdf:first "neutral" ;
                                                                           rdf:rest rdf:nil
                                                                         ]
                                                              ]
                                                   ]
                                        ]
                            ] ;
                 rdfs:comment "The anticolor counterpart to a color charge, relevant for anti-quarks and some gluons; values include anti-red, anti-green, anti-blue." .


###  urn:absolute:OntoSMP#charge
:charge rdf:type owl:DatatypeProperty ;
        rdfs:domain :Particle ;
        rdfs:range owl:real ;
        rdfs:comment "The electric charge of a particle, determining how it interacts electromagnetically. Expressed as multiples of the elementary charge e." .


###  urn:absolute:OntoSMP#colorCharge
:colorCharge rdf:type owl:DatatypeProperty ;
             rdfs:domain :Quark ;
             rdfs:range [ rdf:type rdfs:Datatype ;
                          owl:oneOf [ rdf:type rdf:List ;
                                      rdf:first "blue" ;
                                      rdf:rest [ rdf:type rdf:List ;
                                                 rdf:first "green" ;
                                                 rdf:rest [ rdf:type rdf:List ;
                                                            rdf:first "neutral" ;
                                                            rdf:rest [ rdf:type rdf:List ;
                                                                       rdf:first "red" ;
                                                                       rdf:rest rdf:nil
                                                                     ]
                                                          ]
                                               ]
                                    ]
                        ] ;
             rdfs:comment "The type of color charge carried by a quark or gluon under the SU(3) strong interaction; values include red, green, blue." .


###  urn:absolute:OntoSMP#generation
:generation rdf:type owl:DatatypeProperty ;
            rdfs:domain :Fermion ;
            rdfs:range [ rdf:type rdfs:Datatype ;
                         owl:oneOf [ rdf:type rdf:List ;
                                     rdf:first "I" ;
                                     rdf:rest [ rdf:type rdf:List ;
                                                rdf:first "II" ;
                                                rdf:rest [ rdf:type rdf:List ;
                                                           rdf:first "III" ;
                                                           rdf:rest rdf:nil
                                                         ]
                                              ]
                                   ]
                       ] ;
            rdfs:comment "The classification of fermions into one of three families based on mass and interaction properties (1st, 2nd, or 3rd generation)." .


###  urn:absolute:OntoSMP#lifetime
:lifetime rdf:type owl:DatatypeProperty ;
          rdfs:domain :Particle ;
          rdfs:range xsd:double ;
          rdfs:comment "The average time a particle exists before decaying, typically expressed in seconds." .


###  urn:absolute:OntoSMP#mass
:mass rdf:type owl:DatatypeProperty ;
      rdfs:domain :Particle ;
      rdfs:range owl:real ;
      rdfs:comment "The rest mass of a particle, representing its intrinsic resistance to acceleration. Typically expressed in MeV/c² or GeV/c²." .


###  urn:absolute:OntoSMP#numberOfParticles
:numberOfParticles rdf:type owl:DatatypeProperty ;
                   rdfs:range xsd:unsignedInt .


###  urn:absolute:OntoSMP#reactionType
:reactionType rdf:type owl:DatatypeProperty ;
              rdfs:domain :ChemicalReaction ;
              rdfs:range xsd:string .


###  urn:absolute:OntoSMP#spin
:spin rdf:type owl:DatatypeProperty ;
      rdfs:domain :Particle ;
      rdfs:range xsd:decimal ;
      rdfs:comment "An intrinsic form of angular momentum possessed by particles; values can be integers or half-integers (e.g., 0, 1/2, 1, 3/2)." .


###  urn:absolute:OntoSMP#unit
:unit rdf:type owl:DatatypeProperty ;
      rdfs:subPropertyOf :charge ,
                         :lifetime ,
                         :mass ;
      rdfs:range xsd:string ;
      rdfs:comment "A standard quantity used to express and measure physical properties such as mass, charge, time, or energy." .


